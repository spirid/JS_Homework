{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "off",
    "strict": [
      0,
      "safe"
    ],
    "no-debugger": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "no-trailing-spaces": 2,
    "keyword-spacing": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "spaced-comment": [
      2,
      "always"
    ],
    "no-undef": 0,
    "no-undefined": 0,
    "comma-dangle": [
      2,
      "never"
    ],
    "quotes": [
      0,
      "single"
    ],
    "semi": [
      0,
      "always"
    ],
    "valid-typeof": 2,
    "no-unused-vars": 2,
    "no-extra-semi": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 0,
    "max-len": [
      0,
      120
    ]
  }
}